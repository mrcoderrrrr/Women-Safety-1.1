<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.womenssafety">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/logobg"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logobg"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.WomensSafety">
        <activity android:name=".activity.HomeScreenActivity"></activity>
        <activity android:name=".activity.SplashScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.DetailsPageActivity" />
        <activity android:name=".activity.WelcomePageActivity" />
        <activity android:name=".activity.MainActivity" />
    </application>

</manifest>